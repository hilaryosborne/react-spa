{
  "name": "boilerplate",
  "version": "1.0.0",
  "main": "webpack.config.js",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "linter": "eslint src/**/*.{js,jsx} --ext .js, .jsx",
    "start": "webpack-dev-server --mode development --config webpack.common.js",
    "build:dev": "webpack --mode development -w --env=dev --progress --profile --colors --config webpack.common.js",
    "build:dist": "webpack --mode production --env=prod --progress --profile --colors --config webpack.production.js"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/scripts/testing.js",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "moduleNameMapper": {
      "^images(.*)$": "<rootDir>/src/images$1",
      "^config(.*)$": "<rootDir>/src/config$1",
      "^scripts(.*)$": "<rootDir>/src/scripts$1",
      "^containers(.*)$": "<rootDir>/src/containers$1",
      "^components(.*)$": "<rootDir>/src/components$1",
      "^reducers(.*)$": "<rootDir>/src/reducers$1"
    }
  },
  "dependencies": {
    "bootstrap": "4.0.0",
    "classnames": "^2.2.5",
    "color": "^3.0.0",
    "fbemitter": "^2.1.1",
    "history": "^4.7.2",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "popper.js": "^1.12.9",
    "prop-types": "^15.6.0",
    "radium": "^0.22.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "reactstrap": "^5.0.0-beta",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "superagent": "^3.8.2",
    "superagent-no-cache": "^0.1.1",
    "superagent-throttle": "^0.2.5",
    "validatorjs": "^3.14.2",
    "webpack-cli": "^2.0.10"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^3.0.4",
    "jest": "^22.2.1",
    "jest-enzyme": "^4.2.0",
    "json-loader": "^0.5.7",
    "path": "^0.12.7",
    "react-test-renderer": "^16.2.0",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "url-loader": "^0.6.2",
    "webpack": "^4.1.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dev-server": "^3.1.0",
    "webpack-merge": "^4.1.2"
  }
}
